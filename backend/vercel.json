{
  "git": {
    "deploymentEnabled": {
      "backend": true,
      "main": false
    }
  },
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "ignoreCommand": "bash ignore-build.sh",
  "crons": [
    {
      "path": "/api/cron/reminders",
      "schedule": "0 9 * * *"
    }
  ]
}
